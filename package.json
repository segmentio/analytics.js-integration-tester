{
  "name": "@segment/analytics.js-integration-tester",
  "version": "3.1.1",
  "description": "Helper to quickly test an analytics.js integration.",
  "keywords": [
    "analytics.js",
    "segment"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "make test",
    "lint": "eslint \"./{lib,test}/**/*.js\"",
    "format": "prettier-eslint --write --list-different \"./{lib,test}/**/*.{js,json,md}\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,md}": [
        "prettier-eslint --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/segmentio/analytics.js-integration-tester"
  },
  "author": "Segment <friends@segment.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/segmentio/analytics.js-integration-tester/issues"
  },
  "homepage": "https://github.com/segmentio/analytics.js-integration-tester#readme",
  "dependencies": {
    "@ndhoule/each": "^2.0.1",
    "@ndhoule/includes": "^2.0.1",
    "@ndhoule/keys": "^2.0.0",
    "@segment/fmt": "^1.0.0",
    "@segment/spy": "^1.0.0",
    "@segment/stub": "^1.0.0",
    "component-type": "^1.2.1",
    "domify": "^1.4.0",
    "extend": "^3.0.0",
    "json3": "^3.3.2",
    "proclaim": "^3.4.1"
  },
  "devDependencies": {
    "@segment/analytics.js-core": "^3.0.0",
    "@segment/analytics.js-integration": "^3.1.0",
    "@segment/eslint-config": "^3.1.1",
    "browserify": "^13.0.0",
    "browserify-istanbul": "^2.0.0",
    "codecov": "^3.0.2",
    "component-each": "^0.2.6",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-require-path-exists": "^1.1.8",
    "husky": "^0.14.3",
    "istanbul": "^0.4.3",
    "karma": "1.3.0",
    "karma-browserify": "^5.0.4",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "lint-staged": "^7.2.0",
    "mocha": "^2.2.5",
    "phantomjs-prebuilt": "^2.1.7",
    "prettier-eslint-cli": "^4.7.1",
    "watchify": "^3.7.0"
  }
